Title = "Music Id",
Desc = "Enter Id Music(GamePass Music Required) ", 
Default = "", 
PlaceholderText = "< Input >",
ClearText = false, -- Optional, Clear old Input on focus
Callback = function (value)
playCarMusic(value)
            playScooterMusic(value)
            playHouseMusic(value)
        end
    })

