if selectedPlayerName then
            local targetPlayer = game.Players:FindFirstChild(selectedPlayerName)
            if targetPlayer then
                local soundId = audioID
                local args = {
                    [1] = targetPlayer.Character:FindFirstChild("Head"),
                    [2] = tonumber(soundId),
                    [3] = 1
                }
                game:GetService("ReplicatedStorage").RE:FindFirstChild("1Gu1nSound1s"):FireServer(unpack(args))
                local sound = Instance.new("Sound")
                sound.SoundId = "rbxassetid://" .. soundId
                sound.Parent = targetPlayer.Character.Head
                sound:Play()
                task.wait(3)
                sound:Stop()
            else
MakeNotifi({
Title = "Error" ,
Text = "Player is Not In The Game" ,
Time = 5
}) 
end
else
MakeNotifi({
Title = "Error" ,
Text = "Try Again" ,
Time = 5
}) 
end
end
})

local Toggle = Tab:AddToggle({
Title = "Loop Audio",
Desc = "", 
Callback = function(value)
looping = value
        if looping then
            local function loopAudioGlobal()
                if looping and audioID and tonumber(audioID) then
                    local args = {
                        [1] = game:GetService("Workspace"),
                        [2] = tonumber(audioID),
                        [3] = 1
                    }
                    game:GetService("ReplicatedStorage").RE:FindFirstChild("1Gu1nSound1s"):FireServer(unpack(args))
                    task.wait(3)
                    loopAudioGlobal()
                end
            end
            loopAudioGlobal()
        end
    end
})
