Section = Tab:AddSection({Title = "∣|Custom Car Speed:"})

-- Variables para almacenar Speed y Turbo
local Speed = 50 -- Velocidad por defecto
local Turbo = 50 -- Turbo por defecto

-- Función para cambiar la velocidad y el turbo del carro
local function ChangeCarSpeedAndTurbo(speedValue, turboValue)
    local player = game.Players.LocalPlayer
    local car = workspace.Vehicles:FindFirstChild(player.Name .. "Car")

    if car then
        local body = car:FindFirstChild("Body").VehicleSeat
        if body then
            -- Cambiar la velocidad máxima
            body.TopSpeed.Value = speedValue
            -- Cambiar el valor del turbo
            body.Turbo.Value = turboValue
wait(0.1)
game.StarterGui:SetCore("SendNotification", {
    Title = "Succes";
    Text = "You can move Now!"; 
    Duration = 5;
})
else
game.StarterGui:SetCore("SendNotification", {
    Title = "Error";
    Text = ""; 
    Duration = 5;
})
end
else
game.StarterGui:SetCore("SendNotification", {
    Title = "Error";
    Text = "Put a Car First"; 
    Duration = 5;
})
      end
  end


TextBox = Tab:AddTextBox({
Title = "Change Speed",
Desc = "Change Car Speed", 
Default = "50", 
PlaceholderText = " Input ",
ClearText = false, 
Callback = function(value)
local newSpeed = tonumber(value)
        if newSpeed then
            Speed = newSpeed
end 
end
})

TextBox = Tab:AddTextBox({
Title = "Change Turbo",
Desc = "Change Car Turbo", 
Default = "50", 
PlaceholderText = " Input ",
ClearText = false, 
Callback = function(value)
local newTurbo = tonumber(value)
        if newTurbo then
            Turbo = newTurbo
        else
end
end 
})

