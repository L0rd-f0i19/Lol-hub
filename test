local audioID = ""
local selectedPlayerName = ""
local looping = false
 
local function ReadConfig(config)
    return config
end

TextBox = Tab:AddTextBox({
Title = "Audio Id",
Desc = "Put <font color='rgb(128, 0, 128)'>Audios ID To Troll</font>", 
Default = "", 
PlaceholderText = "Audio ID",
