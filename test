Title = "Select Audio",
Desc = "It Will Be  <font color='rgb(128, 0, 128)'>Selected</font>", -- Optional
Callback = function()
local args = {
            [1] = "PickingTools",
            [2] = "Sniper"
        }
        game:GetService("ReplicatedStorage").RE:FindFirstChild("1Too1l"):InvokeServer(unpack(args))
